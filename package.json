{
  "name": "ffmpeg_nodejs",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "preinstall": "npm i -S google-protobuf node-addon-api nan zeromq through streaming-worker && npm i -D cmake-js && npm run configure && npm run build",
    "configure": "node ./node_modules/cmake-js/bin/cmake-js configure",
    "clean": "node ./node_modules/cmake-js/bin/cmake-js clean",
    "build": "node ./node_modules/cmake-js/bin/cmake-js build",
    "compile": "node ./node_modules/cmake-js/bin/cmake-js compile",
    "rebuild": "node ./node_modules/cmake-js/bin/cmake-js rebuild",
    "test": "node ./test/test_video2images.js"
  },
  "author": "truthbean",
  "license": "MIT",
  "dependencies": {
    "google-protobuf": "^3.6.1",
    "nan": "^2.11.1",
    "node-addon-api": "^1.6.1",
    "streaming-worker": "^1.0.1",
    "through": "^2.3.8",
    "zeromq": "^5.1.0"
  },
  "devDependencies": {
    "cmake-js": "^4.0.1"
  }
}
